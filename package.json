{
  "name": "my-project-workspace",
  "version": "0.1.0",
  "private": true,
  "devDependencies": {
    "@playwright/test": "^1.44.0",
    "@types/node": "^20.11.24",
    "typescript": "^5.3.3"
  },
  "scripts": {
    "install-playwright-browsers": "playwright install --with-deps",
    "test:e2e": "playwright test",
    "test:e2e:ui": "playwright test --ui",
    "test:e2e:headed": "playwright test --headed",
    "test:e2e:debug": "playwright test --debug",
    "codegen": "playwright codegen http://localhost:8080",
    "typecheck": "tsc --noEmit -p jsconfig.json --pretty"
  },
  "engines": {
    "node": ">=18"
  }
}
